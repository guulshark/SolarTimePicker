<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BiDirectionalSeekBar">
        <attr name="BDS_indicatorColor" format="color"/>
        <attr name="BDS_stickColor" format="color" />
        <attr name="BDS_stickGap" format="dimension" />
        <attr name="BDS_zeroStickColor" format="color" />
        <attr name="BDS_labelColor" format="color" />
        <attr name="BDS_minValue" format="integer" />
        <attr name="BDS_maxValue" format="integer" />
        <attr name="BDS_progress" format="integer" />
    </declare-styleable>

    <declare-styleable name="HorizontalWheelView">
        <attr name="tickHeight" format="dimension"/>
        <attr name="tickWidth" format="dimension"/>
        <attr name="tickCount" format="integer"/>
        <attr name="tickColor" format="color"/>
        <attr name="tickDegreesRange" format="integer"/>
        <attr name="tickFadingEdgePercent" format="integer"/>
        <attr name="intermediateTickCount" format="integer"/>
        <attr name="intermediateTickColor" format="color"/>
        <attr name="labelTickColor" format="color"/>
    </declare-styleable>
</resources>